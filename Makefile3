#
# Makefile for ANN - C
#


CC = gcc
CFLAGS = -O -Wall
LFLAGS = -lm -pthread

all: nnet_test3

nnet_test3: test3.c nnet.c nnet.h server.c server.h nnet_io.c nnet_io.h
	$(CC) $(CFLAGS) -c -o nnet.o nnet.c $(LFLAGS)
	$(CC) $(CFLAGS) -c -o server.o server.c $(LFLAGS)
	$(CC) $(CFLAGS) -c -o nnet_io.o nnet_io.c $(LFLAGS)
	$(CC) $(CFLAGS) -c -o test3.o test3.c $(LFLAGS)
	$(CC) $(LFLAGS) -o nnet_test3 test3.o nnet.o server.o nnet_io.o $(LFLAGS)

clean:
	rm -f nnet_io.o server.o nnet.o test3.o nnet_test3